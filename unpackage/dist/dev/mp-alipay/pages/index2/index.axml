<view class="wrapper"><map style="width:100vw;height:100vh;" id="map" latitude="{{latitude}}" longitude="{{longitude}}" scale="{{scale}}" markers="{{markers}}" polyline="{{polyline}}" show-location onRegionChange="scaleChange"></map><view class="btn"><text>{{"里程数 : "+metters}}</text><text>{{'\n时间 : '+time+''}}</text><text>{{"\n配速 : "+speed+''}}</text><button type="default" data-event-opts="{{[['tap',[['start',['$event']]]]]}}" onTap="__e">开始跑步</button><button type="default" data-event-opts="{{[['tap',[['stop',['$event']]]]]}}" onTap="__e">停止跑步</button><button type="default" data-event-opts="{{[['tap',[['clearMsg',['$event']]]]]}}" onTap="__e">结束跑步</button></view></view>